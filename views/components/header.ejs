  <script>
    /**
     * Dismisses global alerts/flash messages
     * Looks for elements with role="alert" or class "alert-dismissible"
     */
    window.dismissAlert = function() {
      const alerts = document.querySelectorAll('[role="alert"], .alert-dismissible');
      alerts.forEach(el => {
        el.style.transition = 'opacity 0.3s ease, transform 0.3s ease';
        el.style.opacity = '0';
        el.style.transform = 'translateY(-10px)';
        setTimeout(() => {
          if (el && el.parentNode) el.remove();
        }, 300);
      });
    };
  </script>

